FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     phtMetalExtractionFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1500;//144000;//108000;//36000;//

deltaT          .000001;//30;//0.1;

writeControl    adjustableRunTime;

writeInterval   30;//3600;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           20;
maxDeltaT       0.01;//.1;//


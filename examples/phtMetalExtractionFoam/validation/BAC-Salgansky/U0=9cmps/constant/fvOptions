// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


energyRelease
{
    type            scalarSemiImplicitSource;

    selectionMode   all;
//	cellZone        heatSource;
	
	h 32;//12;//
	Tinf 300;
	R 0.0125;

	volumeMode      specific;

    injectionRateSuSp
    {
        T.solid           (#eval "2*$h*$Tinf/$R" #eval "-2*$h/$R");
    }
}/**/


